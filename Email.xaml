<Window x:Class="Exam_Unaibayev.Email"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Exam_Unaibayev"
        mc:Ignorable="d"
        Title="Email" Height="350" Width="400">
    <StackPanel  Margin="10">
        <TextBlock Text="Введите Емайл" Margin="10"/>
        <TextBox x:Name="email" Margin="5 0 5 0"></TextBox>
        <Button Content="Назад" Click="Previous_Button" Margin="5"/>
        <Button Content="Вперед" Click="Next_Button" Margin="5"/>
        <Button Content="Перейти в начало" Click="NewResume_Button" Margin="5"/>
        <Button Content="Выход" Click="Close_Button" Margin="5"/>
        <TextBlock HorizontalAlignment="Right" Text="Окно: 5 из 6" Margin="50"/>
    </StackPanel>
</Window>
